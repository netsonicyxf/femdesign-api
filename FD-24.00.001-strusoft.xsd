     <xs:attribute  name="moment_reduction_method"     type="mrm_type"     use="required" />
     <xs:attribute  name="automatic_extent_definition" type="xs:boolean"   default="false" />
     <xs:attribute  name="moment_reduction"            type="xs:boolean"   default="true" />
     <xs:attribute  name="sheart_reduction"            type="xs:boolean"   default="true" />
     <xs:attribute  name="connected_to"                type="guidtype"     use="optional" />
     <xs:attributeGroup ref="entity_attribs" />
     <xs:anyAttribute   processContents="skip"/>
