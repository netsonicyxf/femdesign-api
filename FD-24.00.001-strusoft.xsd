<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="StruSoftXsd"
    targetNamespace="urn:strusoft"
    xmlns="urn:strusoft"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    elementFormDefault="qualified"
    version="01.00.000"
