    </xs:sequence>
    <xs:attribute name="use_default_physical_alignment" type="xs:boolean" use="required" />
    <xs:anyAttribute   processContents="skip"/>
  </xs:complexType>

  <xs:complexType name="truss_eccentricity_type">
    <xs:sequence>
      <xs:element name="physical"   type="ecc_value_type" minOccurs="2" maxOccurs="2" />
    </xs:sequence>
    <xs:attribute name="use_default_physical_alignment" type="xs:boolean" use="required" />
    <xs:anyAttribute   processContents="skip"/>
  </xs:complexType>

  <!-- Connectivity attribs: moves and rotates -->
  <xs:complexType name="connectivity_type">
    <xs:attribute name="predefined_connectivity" type="guidtype" default="00000000-0000-0000-0000-000000000000" />  <!-- from FD24: if this is not default, the other attributes are unused (no need to specify them)! -->

    <xs:attribute name="m_x"         type="xs:boolean"        default="false" /><!-- from FD24: "required" changed to "default" -->
    <xs:attribute name="m_x_release" type="non_neg_max_1e10"  default="0" />    <!-- valid only if m_x is false, from FD15 -->
    <xs:attribute name="m_y"         type="xs:boolean"        default="false" /><!-- from FD24: "required" changed to "default" -->
    <xs:attribute name="m_y_release" type="non_neg_max_1e10"  default="0" />    <!-- valid only if m_y is false, from FD15 -->
    <xs:attribute name="m_z"         type="xs:boolean"        default="false" /><!-- from FD24: "required" changed to "default" -->
    <xs:attribute name="m_z_release" type="non_neg_max_1e10"  default="0" />    <!-- valid only if m_z is false, from FD15 -->
    <xs:attribute name="r_x"         type="xs:boolean"        default="false" /><!-- from FD24: "required" changed to "default" -->
    <xs:attribute name="r_x_release" type="non_neg_max_1e10"  default="0" />    <!-- valid only if r_x is false, from FD15 -->
    <xs:attribute name="r_y"         type="xs:boolean"        default="false" /><!-- from FD24: "required" changed to "default" -->
    <xs:attribute name="r_y_release" type="non_neg_max_1e10"  default="0" />    <!-- valid only if r_y is false, from FD15 -->
    <xs:attribute name="r_z"         type="xs:boolean"        default="false" /><!-- from FD24: "required" changed to "default" -->
    <xs:attribute name="r_z_release" type="non_neg_max_1e10"  default="0" />    <!-- valid only if r_z is false, from FD15 -->
    <xs:anyAttribute   processContents="skip"/>
  </xs:complexType>

  <xs:complexType name="simple_connectivity_type">  <!-- from FD24 -->
    <xs:attribute name="m_x"         type="xs:boolean"        default="false" />
    <xs:attribute name="m_x_release" type="non_neg_max_1e10"  default="0" />    <!-- valid only if m_x is false -->
    <xs:attribute name="m_y"         type="xs:boolean"        default="false" />
    <xs:attribute name="m_y_release" type="non_neg_max_1e10"  default="0" />    <!-- valid only if m_y is false -->
    <xs:attribute name="m_z"         type="xs:boolean"        default="false" />
    <xs:attribute name="m_z_release" type="non_neg_max_1e10"  default="0" />    <!-- valid only if m_z is false -->
    <xs:attribute name="r_x"         type="xs:boolean"        default="false" />
    <xs:attribute name="r_x_release" type="non_neg_max_1e10"  default="0" />    <!-- valid only if r_x is false -->
    <xs:attribute name="r_y"         type="xs:boolean"        default="false" />
    <xs:attribute name="r_y_release" type="non_neg_max_1e10"  default="0" />    <!-- valid only if r_y is false -->
    <xs:attribute name="r_z"         type="xs:boolean"        default="false" />
    <xs:attribute name="r_z_release" type="non_neg_max_1e10"  default="0" />    <!-- valid only if r_z is false -->
