  </xs:simpleType>

  <xs:simpleType name="general_material_nu_value">
    <xs:restriction base="xs:double">
      <xs:minInclusive value="0.001"/>
      <xs:maxInclusive value="0.499"/>
    </xs:restriction>
  </xs:simpleType>

