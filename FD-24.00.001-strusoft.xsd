     <xs:attribute  name="t_right"                     type="t_data_type"  use="required" />
     <xs:anyAttribute   processContents="skip"/>
  </xs:complexType>

  <xs:complexType name="beam_reduction_zone_type" >
     <xs:sequence>
        <xs:element name="position"                    type="point_type_3d"/>
