    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="sf_rc_face">
    <xs:restriction base="xs:string">
      <xs:enumeration value="top"/>
      <xs:enumeration value="bottom"/>
    </xs:restriction>
  </xs:simpleType>
  <!--pre defined section types-->
